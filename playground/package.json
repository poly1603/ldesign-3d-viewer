{
  "name": "@ldesign/3d-viewer-playground",
  "version": "1.0.0",
  "private": true,
  "description": "Playground demo for @ldesign/3d-viewer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && copy vercel.json dist\\vercel.json && cd dist && vercel",
