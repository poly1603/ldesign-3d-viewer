{
  "name": "@ldesign/3d-viewer-vue",
  "version": "1.0.0",
  "description": "Vue 3 components for @ldesign/3d-viewer",
  "author": "LDesign Team",
  "license": "MIT",
  "keywords": [
    "panorama",
    "vue",
    "vue3",
    "3d"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/PanoramaViewer.vue.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && pnpm build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@ldesign/3d-viewer": "workspace:*",
    "three": "^0.160.0",
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@ldesign/3d-viewer": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.0",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.3.0",
    "@vue/test-utils": "^2.4.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "happy-dom": "^12.10.0",
    "jsdom": "^23.0.0",
    "rimraf": "^5.0.5",
    "three": "^0.160.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.1.0",
    "vue": "^3.3.0",
    "vue-tsc": "^3.1.2"
  }
}
