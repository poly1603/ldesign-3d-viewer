{
  "name": "@panorama-viewer/lit",
  "version": "1.0.0",
  "description": "Lit Web Component wrapper for panorama viewer",
  "author": "",
  "license": "MIT",
  "keywords": [
    "panorama",
    "lit",
    "web-component",
    "3d"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@panorama-viewer/core": "workspace:*",
    "lit": "^3.0.0",
    "three": "^0.160.0"
  },
  "dependencies": {
    "@panorama-viewer/core": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.0",
    "@types/three": "^0.160.0",
    "eslint": "^8.56.0",
    "lit": "^3.1.0",
    "rimraf": "^5.0.5",
    "three": "^0.160.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
