    "@ldesign/3d-viewer-vue": "workspace:*",
    "lucide-vue-next": "^0.562.0",
    "three": "^0.160.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.3.3",
    "vercel": "^39.0.0",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  }
}
