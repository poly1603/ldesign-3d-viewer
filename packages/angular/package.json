{
  "name": "@ldesign/3d-viewer-angular",
  "type": "module",
  "version": "1.0.0",
  "description": "Angular wrapper for 3D Panorama Viewer",
  "author": "",
  "license": "MIT",
  "keywords": [
    "panorama",
    "3d",
    "viewer",
    "angular",
    "threejs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0",
    "@panorama-viewer/core": "workspace:*",
    "three": "^0.160.0"
  },
  "dependencies": {
    "@panorama-viewer/core": "workspace:*"
  },
  "devDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@antfu/eslint-config": "^2.6.0",
    "@types/three": "^0.160.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "three": "^0.160.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "zone.js": "^0.14.2"
  }
}
